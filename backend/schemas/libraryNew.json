{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "libraryData",
    "USContact",
    "PHContact",
    "primaryAddress",
    "readingSpaces",
    "adminId"
  ],
  "properties": {
    "libraryData": {
      "type": "object",
      "default": {},
      "title": "The libraryData Schema",
      "required": [
        "libraryName",
        "libraryType",
        "program"
      ],
      "properties": {
        "libraryName": {
          "type": "string",
          "default": "",
          "title": "The libraryName Schema",
          "examples": [
            "LibraryName"
          ]
        },
        "libraryType": {
          "type": "string",
          "default": "",
          "title": "The libraryType Schema",
          "examples": [
            "elementary school"
          ]
        },
        "classrooms": {
          "member_region": {
            "type": [
              "integer",
              "null"
            ]
          },
          "default": 0,
          "title": "The classrooms Schema",
          "examples": [
            3
          ]
        },
        "studentsPerGrade": {
          "member_region": {
            "type": [
              "integer",
              "null"
            ]
          },
          "default": 0,
          "title": "The studentsPerGrade Schema",
          "examples": [
            18
          ]
        },
        "teachers": {
          "member_region": {
            "type": [
              "integer",
              "null"
            ]
          },
          "default": 0,
          "title": "The teachers Schema",
          "examples": [
            7
          ]
        },
        "program": {
          "type": "string",
          "default": "",
          "title": "The program Schema",
          "examples": [
            "FSER"
          ]
        }
      },
      "examples": [
        {
          "libraryName": "LibraryName",
          "libraryType": "elementary school",
          "classrooms": 3,
          "studentsPerGrade": 18,
          "teachers": 7,
          "program": "FSER"
        }
      ]
    },
    "USContact": {
      "type": "object",
      "default": {},
      "title": "The USContact Schema",
      "required": [
        "firstName",
        "lastName",
        "email",
        "phone"
      ],
      "properties": {
        "firstName": {
          "type": "string",
          "default": "",
          "title": "The firstName Schema",
          "examples": [
            "FirstName"
          ]
        },
        "lastName": {
          "type": "string",
          "default": "",
          "title": "The lastName Schema",
          "examples": [
            "LastName"
          ]
        },
        "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": [
            "name@email.com"
          ]
        },
        "phone": {
          "type": "string",
          "default": "",
          "title": "The phone Schema",
          "examples": [
            "1111111111"
          ]
        }
      },
      "examples": [
        {
          "firstName": "FirstName",
          "lastName": "LastName",
          "email": "name@email.com",
          "phone": "1111111111"
        }
      ]
    },
    "PHContact": {
      "type": "object",
      "default": {},
      "title": "The PHContact Schema",
      "required": [
        "firstName",
        "lastName",
        "email",
        "phone"
      ],
      "properties": {
        "firstName": {
          "type": "string",
          "default": "",
          "title": "The firstName Schema",
          "examples": [
            "FirstName"
          ]
        },
        "lastName": {
          "type": "string",
          "default": "",
          "title": "The lastName Schema",
          "examples": [
            "LastName"
          ]
        },
        "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": [
            "name@email.com"
          ]
        },
        "phone": {
          "type": "string",
          "default": "",
          "title": "The phone Schema",
          "examples": [
            "1111111111"
          ]
        }
      },
      "examples": [
        {
          "firstName": "FirstName",
          "lastName": "LastName",
          "email": "name@email.com",
          "phone": "1111111111"
        }
      ]
    },
    "primaryAddress": {
      "type": "object",
      "default": {},
      "title": "The primaryAddress Schema",
      "required": [
        "street",
        "barangay",
        "city",
        "regionId",
        "provinceId"
      ],
      "properties": {
        "street": {
          "type": "string",
          "default": "",
          "title": "The street Schema",
          "examples": [
            "1 street address "
          ]
        },
        "barangay": {
          "type": "string",
          "default": "",
          "title": "The barangay Schema",
          "examples": [
            "barangay"
          ]
        },
        "city": {
          "type": "string",
          "default": "",
          "title": "The city Schema",
          "examples": [
            "city"
          ]
        },
        "regionId": {
          "type": "integer",
          "default": 0,
          "title": "The regionId Schema",
          "examples": [
            3
          ]
        },
        "provinceId": {
          "type": "integer",
          "default": 0,
          "title": "The provinceId Schema",
          "examples": [
            67
          ]
        }
      },
      "examples": [
        {
          "street": "1 street address ",
          "barangay": "barangay",
          "city": "city",
          "regionId": 3,
          "provinceId": 67
        }
      ]
    },
    "readingSpaces": {
      "type": "array",
      "default": [],
      "title": "The readingSpaces Schema",
      "items": {
        "type": "string",
        "title": "A Schema",
        "examples": [
          "reading corner",
          "dedicated reading space"
        ]
      },
      "examples": [
        [
          "reading corner",
          "dedicated reading space"
        ]
      ]
    },
    "adminId": {
      "type": "integer",
      "default": 0,
      "title": "The adminId Schema",
      "examples": [
        45
      ]
    }
  },
  "additionalProperties": false,
  "examples": [
    {
      "libraryData": {
        "libraryName": "LibraryName",
        "libraryType": "elementary school",
        "classrooms": 3,
        "studentsPerGrade": 18,
        "teachers": 7,
        "program": "FSER"
      },
      "contactData": {
        "firstName": "FirstName",
        "lastName": "LastName",
        "email": "name@email.com",
        "phone": "1111111111"
      },
      "primaryAddress": {
        "street": "1 street address ",
        "barangay": "barangay",
        "city": "city",
        "regionId": 3,
        "provinceId": 67
      },
      "readingSpaces": [
        "reading corner",
        "dedicated reading space"
      ],
      "adminId": 45
    }
  ]
}