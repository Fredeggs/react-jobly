{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/userAuth.json",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "isAdmin": {
      "type": "boolean"
    },
    "id": {
      "type": "number"
    },
    "libraryId": {
      "type": "number"
    }
  },
  "additionalProperties": true,
  "required": [
    "email",
    "firstName",
    "lastName",
    "phone",
    "isAdmin",
    "id",
    "libraryId"
  ]
}